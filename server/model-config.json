{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins"
    ]
  },
  "User": {
    "dataSource": "mongodb",
    "public": false
  },
  "AccessToken": {
    "dataSource": "mongodb",
    "public": false
  },
  "ACL": {
    "dataSource": "mongodb",
    "public": false
  },
  "RoleMapping": {
    "dataSource": "mongodb",
    "public": false,
    "options": {
      "strictObjectIDCoercion": true
    }
  },
  "Role": {
    "dataSource": "mongodb",
    "public": false
  },
  "Image": {
    "dataSource": "transient",
    "public": false
  },
  "Author": {
    "dataSource": "mongodb",
    "public": true
  },
  "Reader": {
    "dataSource": "mongodb",
    "public": true
  },
  "Post": {
    "dataSource": "mongodb",
    "public": true
  },
  "Locale": {
    "dataSource": "transient",
    "public": false
  },
  "Comment": {
    "dataSource": "mongodb",
    "public": true
  },
  "Blog": {
    "dataSource": "mongodb",
    "public": true
  },
  "Analytics": {
    "dataSource": "redis",
    "public": true
  }
}
